{
  "name": "f-flat",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "bin": "./dist/repl.js",
  "scripts": {
    "rollup:bin": "rollup -c rollup.repl.js > dist/repl.js",
    "rollup:lib": "rollup -c rollup.lib.js > dist/stack.js",
    "compile": "npm run rollup:bin && npm run rollup:lib",
    "test": "ava",
    "lint": "xo",
    "check": "npm run coverage && npm run lint",
    "coverage": "nyc -r lcov -r html -r text ava",
    "start": "babel-node ./bin/repl.js"
  },
  "author": "J. Harshbarger",
  "license": "MIT",
  "dependencies": {
    "compute-erf": "^3.0.3",
    "decimal.js": "^5.0.0",
    "fantasy-helpers": "0.0.1",
    "isomorphic-fetch": "^2.2.1",
    "mini-signals": "^1.1.1",
    "progress": "^1.1.8",
    "rfr": "^1.2.3",
    "typed-function": "^0.10.3",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-cli": "^6.5.1",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "eslint": "^2.2.0",
    "eslint-plugin-ava": "^1.0.0",
    "kneden": "^1.0.0",
    "nock": "^7.0.2",
    "nyc": "^5.6.0",
    "rollup": "^0.25.3",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-json": "^2.0.0",
    "xo": "^0.13.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "kneden"
    ]
  },
  "ava": {
    "files": [
      "test/*-spec.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "xo": {
    "esnext": true,
    "semicolon": true,
    "space": 2,
    "space-before-function-paren": [
      2,
      {
        "anonymous": "always",
        "named": "always"
      }
    ],
    "rules": {
      "space-before-function-paren": [2, {"anonymous": "always", "named": "always"}],
      "no-warning-comments": 0,
      "prefer-reflect": 2,
      "ava/max-asserts": [2, 5],
      "ava/no-cb-test": 0,
      "ava/no-identical-title": 2,
      "ava/no-invalid-end": 2,
      "ava/no-only-test": 2,
      "ava/no-skip-assert": 2,
      "ava/no-skip-test": 2,
      "ava/prefer-power-assert": 0,
      "ava/test-ended": 2,
      "ava/test-title": [2, "always"]
    },
    "plugins": [
      "ava"
    ]
  }
}
