{
  "name": "f-flat",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "bin": "./dist/repl.js",
  "scripts": {
    "rollup:bin": "rollup -c rollup.repl.js > dist/repl.js",
    "rollup:lib": "rollup -c rollup.lib.js > dist/stack.js",
    "compile": "npm run rollup:bin && npm run rollup:lib",
    "test": "babel-node ./test/f-flat-spec.js",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover --report lcov --report html --report text ./test/f-flat-spec.js",
    "start": "babel-node ./bin/repl.js"
  },
  "author": "J. Harshbarger",
  "license": "MIT",
  "dependencies": {
    "compute-erf": "^3.0.3",
    "decimal.js": "^5.0.0",
    "fantasy-helpers": "0.0.1",
    "isomorphic-fetch": "^2.2.1",
    "mini-signals": "^1.1.1",
    "progress": "^1.1.8",
    "rfr": "^1.2.3",
    "typed-function": "^0.10.3",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "isparta": "^4.0.0",
    "kneden": "^1.0.0",
    "nock": "^7.0.2",
    "rollup": "^0.25.3",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-json": "^2.0.0",
    "tape": "^4.4.0"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "extends": [
      "semistandard"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "kneden"
    ]
  }
}
