{
  "name": "f-flat",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "bin": "./dist/repl.js",
  "scripts": {
    "rollup:bin": "rollup -c rollup.repl.js > dist/repl.js",
    "rollup:lib": "rollup -c rollup.lib.js > dist/stack.js",
    "compile": "npm run rollup:bin && npm run rollup:lib",
    "test": "ava",
    "lint": "xo",
    "check": "npm run coverage && npm run lint",
    "coverage": "nyc -r lcov -r html -r text ava",
    "start": "babel-node ./bin/repl.js"
  },
  "author": "J. Harshbarger",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "compute-erf": "^3.0.3",
    "decimal.js": "^5.0.8",
    "fantasy-helpers": "0.0.1",
    "fixed-width-string": "^1.0.0",
    "icepick": "^1.2.0",
    "isomorphic-fetch": "^2.2.1",
    "literalizer": "^0.4.0-a",
    "memoizee": "^0.3.9",
    "mini-signals": "^1.1.1",
    "progress": "^1.1.8",
    "rfr": "^1.2.3",
    "typed-function": "^0.10.4",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.5",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "eslint": "^2.7.0",
    "kneden": "^1.0.4",
    "nock": "^8.0.0",
    "nyc": "^6.2.1",
    "rollup": "^0.25.8",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-json": "^2.0.0",
    "xo": "^0.14.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "kneden"
    ]
  },
  "ava": {
    "files": [
      "test/*-spec.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "xo": {
    "esnext": true,
    "semicolon": true,
    "space": 2,
    "rules": {
      "space-before-function-paren": [
        2,
        {
          "anonymous": "always",
          "named": "always"
        }
      ],
      "spaced-comment": 0,
      "no-warning-comments": 0,
      "prefer-reflect": 2
    }
  }
}
