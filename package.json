{
  "name": "f-flat",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "bin": "./src/repl.js",
  "scripts": {
    "test": "ava",
    "lint": "eslint src/** bin/** test/**",
    "check": "npm run coverage && npm run lint",
    "coverage": "nyc -r lcov -r html -r text ava",
    "start": "node -r @std/esm ./bin/repl.mjs"
  },
  "author": "J. Harshbarger",
  "license": "MIT",
  "dependencies": {
    "@std/esm": "^0.13.0",
    "chalk": "^1.1.3",
    "commander": "^2.11.0",
    "compute-erf": "^3.0.3",
    "decimal.js": "^5.0.8",
    "fantasy-helpers": "0.0.1",
    "fixed-width-string": "^1.0.0",
    "icepick": "^1.2.0",
    "isomorphic-fetch": "^2.2.1",
    "literalizer": "^0.4.0-a",
    "memoizee": "^0.3.9",
    "mini-signals": "^1.1.1",
    "progress": "^1.1.8",
    "rfr": "^1.2.3",
    "typed-function": "^0.10.4",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "eslint": "^2.13.1",
    "kneden": "^1.0.4",
    "nock": "^8.0.0",
    "nyc": "^6.2.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "kneden"
    ]
  },
  "ava": {
    "files": [
      "test/*-spec.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "xo": {
    "esnext": true,
    "semicolon": true,
    "space": 2,
    "rules": {
      "space-before-function-paren": [
        2,
        {
          "anonymous": "always",
          "named": "always"
        }
      ],
      "spaced-comment": 0,
      "no-warning-comments": 0,
      "prefer-reflect": 2
    }
  },
  "@std/esm": {
    "esm": "all",
    "cjs": true
  }
}
