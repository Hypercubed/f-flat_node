/**
 * ## `;` ( define action )
 * Converts the quote into an action and stores
 */
[ : swap sto ] : ';' sto

/**
 * ## `;;` ( define inline action )
 * Converts the quote into an inline action and stores
 */
;;: [ expand ; ] expand ;

/**
 * ## `in`
 */
in: [ ( swap eval ) ] ;

/**
 * ## `map`
 */
map: [ * in ] ;;
