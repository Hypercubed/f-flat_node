/**
 * # Classic combinators
 */
 
'core.ff' import
'shuffle.ff' import

x: [ dup eval ] ;
i: [ eval ] ;
?: [ dup [ [ '_?' eval ] >> ] dip ] ;
w: [ dupd eval ] ;
b: [ [ >> ] dip eval ] ;
c: [ swapd eval ] ;
k: [ dropd eval ] ;
t: [ swap eval ] ;
s: [ [ over >> swap ] dip eval ] ;
y: [ [ dup >> ] swap + dup >> eval ] ;
